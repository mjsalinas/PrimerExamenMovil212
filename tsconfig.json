{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "target": "ESNext",
    "moduleResolution": "bundler",
    "customConditions": ["react-native"],
    "strict": true,
    "allowJs": false,
    "jsx": "react-native",
    "resolveJsonModule": true,
    "esModuleInterop": true,
    "skipLibCheck": true
  }
}
